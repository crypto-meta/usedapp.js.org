{
  "private": true,
  "name": "dumi-app",
  "scripts": {
    "start": "dumi dev",
    "build": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\""
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crypto-meta/usedapp.js.org.git",
    "branch": "main",
    "platform": "github"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@luozhu/eslint-config-react-typescript": "^1.5.4",
    "@luozhu/prettier-config": "^1.2.0",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "dumi": "^1.0.13",
    "eslint": "^8.5.0",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@usedapp/core": "0.7.1",
    "antd": "^4.17.4"
  }
}
